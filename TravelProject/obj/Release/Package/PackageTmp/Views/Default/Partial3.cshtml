@using TravelProject.Models.Sınıflar
@model List<Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.blogImage" alt="" style="width:120px; height:65px;" />
            </div>
            <div class="bot-desc">
                <p>@item.aciklama.Substring(0,70) ...</p>
            </div>
            <div class="clear"></div>
            
        }
    </div>
    @Html.Action("Partial4","Default")
</div>

