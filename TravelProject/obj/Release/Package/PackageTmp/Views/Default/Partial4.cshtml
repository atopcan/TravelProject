@using TravelProject.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.blogImage" alt="" style="width:120px; height:65px;" />
        </div>
        <div class="bot-desc1">
            <p>@item.aciklama.Substring(0,70)</p>
        </div>
        <div class="clear"></div>
    }

</div>

